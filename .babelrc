{
  "presets": ["react", "es2015", "stage-0"],
  "plugins": [
    [
        "css-modules-transform", {
            "generateScopedName": "[local]",
            "extensions": [".css", ".scss"]
        }
    ],
    [
        "transform-assets", {
            "extensions": ["jpg", "jpeg", "gif", "png", "ico", "svg"],
            "name": "/[name].[ext]"
        }
    ]
]
}